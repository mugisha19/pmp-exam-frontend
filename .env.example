# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# API Configuration
VITE_API_URL=http://localhost:8000/api/v1

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=
VITE_APPLE_CLIENT_ID=

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8000

# Environment
VITE_ENV=development

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_APPLE_AUTH=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_WEBSOCKET=true

# App Configuration
VITE_APP_NAME=PMP Exam Platform
VITE_APP_VERSION=1.0.0

# API Timeouts (in milliseconds)
VITE_API_TIMEOUT=30000
VITE_UPLOAD_TIMEOUT=60000

# Pagination
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# Quiz Configuration
VITE_QUIZ_AUTO_SAVE_INTERVAL=30000
VITE_QUIZ_HEARTBEAT_INTERVAL=60000
VITE_QUIZ_INACTIVITY_WARNING=300000

# File Upload Limits
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=.jpg,.jpeg,.png,.pdf,.doc,.docx

# Cache TTL (in seconds)
VITE_CACHE_TTL=300

# Debug Mode
VITE_DEBUG=false
VITE_LOG_LEVEL=info
